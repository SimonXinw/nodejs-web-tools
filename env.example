# Supabase 配置 (必需)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# 爬虫配置 (可选)
SCRAPER_HEADLESS=true
SCRAPER_TIMEOUT=30000
SCRAPER_RETRY_COUNT=3

# 定时任务配置 (可选)
GOLD_PRICE_SCHEDULE=0 * * * *

# API 服务器配置 (可选)
ENABLE_API=false
API_PORT=3000

# 日志配置 (可选)
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# 运行环境 (可选)
NODE_ENV=development
